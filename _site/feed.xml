})
@RouteConfig([
  {
     path: &#39;/&#39;,
     redirectTo: [&#39;/Users&#39;, &#39;UsersList&#39;]
  },
  {
    path: &#39;/users/...&#39;,
    component: UsersRoute,
    as: &#39;Users&#39;
  }
])
export class App {

}&lt;/code&gt;&lt;/pre&gt;&lt;/figure&gt;

